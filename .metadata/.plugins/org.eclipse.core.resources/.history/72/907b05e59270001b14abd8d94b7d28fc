package com.R177219041.q1;

import java.util.Scanner;

public class Combo {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int Arr[] = new int[3];
		
		System.out.print("Enter the digits : ");
		for(int i=0;i<3;i++) {
			Arr[i] = sc.nextInt();
		}	
	
		System.out.println(Arr[0]);
		

}
}
