package com.R177219041.q2;

import java.util.Scanner;

public class SquareSum {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int Arr[] = new int[10];
		int sum=0;
		
		System.out.print("Enter the 10 numbers : ");
		for(int i=0;i<2;i++) {
			Arr[i] = sc.nextInt();
		}
		
		for(int i=0;i<10;i++) {
			sum =sum+(Arr[i]^2);
		}

	}

}
